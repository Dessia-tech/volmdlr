  <content>
    <workbench>
      <classname>Note: Missing InitGui.py</classname> <!-- Must match class name in InitGui.py -->
      <subdirectory>./path/to/classname</subdirectory>

      <depend>binaryornot</depend>
      <depend>cython</depend>
      <depend version_lte="0.7.0">dessia-common</depend>
      <depend>geomdl</depend>
      <depend>gmsh</depend>
      <depend>matplotlib</depend>
      <depend>numpy</depend>
      <depend>scipy</depend>
      <depend>jsonschema</depend>
      <depend>networkx</depend>
      <depend version_lte="0.10.9">plot_data</depend>
      <depend>rtree</depend>
      <depend>kaitaistruct</depend>
      <depend>sympy</depend>
      <depend>trimesh</depend>
      <depend>triangle</depend>

      <tag>volume modeler</tag>
      <tag>python</tag>
      <tag>step</tag>
    </workbench>
  </content>
</package>
